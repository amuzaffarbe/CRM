<!DOCTYPE html>
<html lang="en">

<head>
     <title>Login V1</title>
     <meta charset="UTF-8">
     <meta name="viewport" content="width=device-width, initial-scale=1">
     <link rel="stylesheet" type="text/css" href="css/app.css">
</head>

<body>
     <div class="limiter">
          <div class="container-login100">
               <div class="wrap-login100">
                    <div class="login100-pic js-tilt" data-tilt>
                         <img src="images/img-01.png" alt="IMG">
                    </div>
                    <form class="login100-form validate-form" action="/" enctype="application/x-www-form-urlencoded" method="POST"  >
                         <span class="login100-form-title">
                              Member Login
                         </span>
                         <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                              <input class="input100" type="text" name="username" placeholder="Username">
                              <span class="focus-input100"></span>
                              <span class="symbol-input100">
                                   <i class="fa fa-envelope" aria-hidden="true"></i>
                              </span>
                         </div>
                         <div class="wrap-input100 validate-input" data-validate="Password is required">
                              <input class="input100" type="password" name="password" placeholder="Password">
                              <span class="focus-input100"></span>
                              <span class="symbol-input100">
                                   <i class="fa fa-lock" aria-hidden="true"></i>
                              </span>
                         </div>
                         <div class="container-login100-form-btn">
                              <button type="submit" class="login100-form-btn">
                                   Login
                              </button>
                              <button id="getadmin">Get admin login</button>
                         </div>
                         
                        
                    </form>
               </div>
          </div>
     </div>

     <script>
          let getadmin = document.querySelector("#getadmin");

          getadmin.addEventListener("click", (e) => {
               e.preventDefault()
               alert("\nUsername:  Muzaffarbek \nPassword:    7777")
          })
     </script>script>
</body>

</html>